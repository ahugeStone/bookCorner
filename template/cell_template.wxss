.cell_item {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 66px;
  background-color: #FFF;
  width: 100%;
  margin: 0 auto;
  border-bottom: 1px solid #F3F3F3;
  flex-shrink: 0;
}
.cell_left {
  display: flex;
  width: 40px;
  height: 40px;
  background-color: #333333;
  margin-left: 5%;
}
.cell_left image {
  width: 100%;
  height: 100%;
}
.cell_right {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 5%;
  margin-right: 5%;
}
.cell_title {
  display: flex;
  font-size: 16px;
  color: #222222;
  width: 100%;
}
.cell_sub_title {
  display: flex;
  font-size: 14px;
  color: #999999;
  width:100%;
}
.img_book {
  background-image: url(https://shanximokey.cn/images/book.png);
  background-repeat: no-repeat;
  background-size: cover;
}
