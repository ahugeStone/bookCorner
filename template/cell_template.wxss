.cell_item {
  display: flex;
  flex-direction: row;
  align-items: center;
  /* height: 66px; */
  background-color: #FFF;
  width: 100%;
  margin: 0 auto;
  border-bottom: 1px solid #F3F3F3;
  flex-shrink: 0;
}
.cell_left {
  display: flex;
  flex: none;
  width: 96px;
  height: 96px;
  background-color: #333333;
  margin-left: 5%;
}
.cell_left image {
  width: 100%;
  height: 100%;
}
.cell_right {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 6px;
  margin-right: 5%;
}
.cell_title {
  display: flex;
  font-size: 16px;
  color: #222222;
  width: 100%;
  margin-top:14px;
}
.cell_sub_title {
  display: flex;
  font-size: 15px;
  color: #999999;
  width:100%;
  margin-top:6px;
}
.img_book {
  background-image: url(https://shanximokey.cn/images/book.png);
  background-repeat: no-repeat;
  background-size: cover;
}
.cell_right_bottom{
  display:flex;

}
.thumbup_white{
  width:13px;
  height:11px;
  margin-top:9px;
  margin-bottom:14px;
}
.cell_right_bottom text{
  font-size: 12px;
  margin-top:5px;
  margin-left:7px;
  margin-right:7px;
}